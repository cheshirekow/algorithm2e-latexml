<grammar ns="http://dlmf.nist.gov/LaTeXML" 
        xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0" 
        xmlns="http://relaxng.org/ns/structure/1.0" 
        datatypeLibrary="http://www.w3.org/2001/XMLSchema-datatypes">
  <!-- include the original schema -->
  <!--include href="/home/josh/Codes/devroot/usr/share/perl/5.10.1/LaTeXML/schema/RelaxNG/LaTeXML.rng"/  -->
  <!--include href="/usr/share/perl5/LaTeXML/schema/RelaxNG/LaTeXML.rng"/-->
  <include href="/usr/share/perl5/LaTeXML/schema/RelaxNG/LaTeXML.rng"/>
  <define name="Para.class" combine="choice">
    <choice>
      <ref name="Algorithm.algorithm"/>
    </choice>
  </define>
  <define name="Algorithm.algorithm" >
    <element name="algorithm" ns="http://dlmf.nist.gov/LaTeXML/packages/algorithm2e" >
      <a:documentation>A logical block composing an algorithm</a:documentation>
      <ref name="Common.attributes"/>
      <ref name="Labelled.attributes"/>
      <ref name="Caption.class"/>
      <ref name="Algorithm.caption"/>
      <ref name="Algorithm.block"/>
    </element>
  </define>
  <define name="Algorithm.caption">
    <element name="caption" ns="http://dlmf.nist.gov/LaTeXML/packages/algorithm2e" >
      <ref name="Common.attributes"/>
      <ref name="Labelled.attributes"/>
      <ref name="Inline.model"/>
    </element>
  </define>
  <define name="Algorithm.block">
    <element name="block" ns="http://dlmf.nist.gov/LaTeXML/packages/algorithm2e" >
      <a:documentation>A logical block which is composed of other logical blocks</a:documentation>
      <ref name="Common.attributes"/>
      <attribute name="depth"/>
      <ref name="Algorithm.line"/>
      <ref name="Algorithm.block"/>
    </element>
  </define>
  <define name="Algorithm.line">
    <element name="line" ns="http://dlmf.nist.gov/LaTeXML/packages/algorithm2e">
      <a:documentation>A logical block composing one line of an algorithm</a:documentation>
      <ref name="Common.attributes"/>
      <ref name="Labelled.attributes"/>
      <ref name="Inline.model"/>
    </element>
  </define>
</grammar>
